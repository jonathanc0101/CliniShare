{
  "name": "my-electron-app",
  "version": "1.0.0",
  "description": "hello world app",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "dev": "nodemon backend/src/main.js",
    "backend": "node backend/src/main.js",
    "rcb": "npm run --prefix frontend/ start",
    "rc": "BROWSER=none npm run --prefix frontend/ start",
    "most": "concurrently -k \"npm:rcb\" \"npm:dev\"",
    "all": "concurrently -k \" npm run rc\" \"npm:electron\" \"npm:dev\"",
    "prod": "concurrently -k \" npm run rc\" \"npm:electron\" \"npm:backend\"",
    "docker": "concurrently \"docker-compose build && docker-compose up\" \"npm:electron\" ",
    "after-docker": "concurrently \"docker-compose up\" \"npm:electron\" ",
    "compose-backends": "docker-compose build backend backend2 backend3 postgres postgres2 postgres3 && docker-compose up",
    "electron": "wait-on tcp:5000 && electron .",
    "electron2": "electron ."
},
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.65",
    "@electron-forge/maker-deb": "^6.0.0-beta.65",
    "@electron-forge/maker-rpm": "^6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.65",
    "@electron-forge/maker-zip": "^6.0.0-beta.65",
    "electron": "^20.1.0",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "broadcast-address": "^1.0.2",
    "concurrently": "^7.5.0",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.6",
    "dgram": "^1.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.1",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "react-window": "^1.8.7",
    "sequelize": "^6.23.2",
    "sweetalert2": "^11.5.1",
    "wait-on": "^6.0.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "my_electron_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
